generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/userlient"
}

datasource db {
  provider = "mysql"
  url      = env("USER_DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(30)
  email    String @db.VarChar(60)
  password String @db.VarChar(255)
  role     Int
}
