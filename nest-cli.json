{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/msa-nestjs/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/msa-nestjs/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/msa-nestjs",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "board": {
      "type": "application",
      "root": "apps/board",
      "entryFile": "main",
      "sourceRoot": "apps/board/src",
      "compilerOptions": {
        "tsConfigPath": "apps/board/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "graphql-gateway": {
      "type": "application",
      "root": "apps/graphql-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/graphql-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql-gateway/tsconfig.app.json"
      }
    },
    "redis-cache": {
      "type": "application",
      "root": "apps/redis-cache",
      "entryFile": "main",
      "sourceRoot": "apps/redis-cache/src",
      "compilerOptions": {
        "tsConfigPath": "apps/redis-cache/tsconfig.app.json"
      }
    },
    "oauth": {
      "type": "application",
      "root": "apps/oauth",
      "entryFile": "main",
      "sourceRoot": "apps/oauth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/oauth/tsconfig.app.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    }
  }
}